                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="0">Proficiency</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="1">Wield-Template</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="2">Range/Incr</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="3">Max-Str/Hands</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="4">Damage</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="5">Enhancement</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="6">Attack/Damage</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="7">Critical Low</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="8">Critical Damage</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="10">Material</TextBlock>

                    <TextBlock Grid.Column="1" Grid.Row="0" Text="{Binding Path=Thing.ProficiencyType}"/>
                    <TextBlock Grid.Column="1" Grid.Row="1" Text="{Binding Path=Thing.WieldTemplate}"/>
                    <TextBlock Grid.Column="1" Grid.Row="2">
                        <Run Text="{Binding Path=Thing.MaxRange}"/>
                        <Run Text="/"/>
                        <Run Text="{Binding Path=Thing.RangeIncrement}"/>
                    </TextBlock>
                    <StackPanel Orientation="Horizontal" Grid.Column="1" Grid.Row="3">
                        <TextBlock Text="{Binding Path=Thing.MaxStrengthBonus}"/>
                    </StackPanel>

                    <TextBlock Grid.Column="1" Grid.Row="4" Text="{Binding Path=Thing.MediumDamageRollString}"/>
                    <StackPanel Orientation="Horizontal" Grid.Column="1" Grid.Row="5">
                        <TextBlock Text="{Binding Path=Thing.ListedEnhancement}"/>
                        <TextBlock Text="/"/>
                        <ContentControl Content="{Binding Path=Thing.TotalEnhancement}" ContentTemplate="{StaticResource deltaView}"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Grid.Column="1" Grid.Row="6">
                        <ContentControl Content="{Binding Path=Thing.AttackBonus}" ContentTemplate="{StaticResource deltaView}"/>
                        <TextBlock Text="/"/>
                        <ContentControl Content="{Binding Path=Thing.DamageBonus}" ContentTemplate="{StaticResource deltaView}"/>
                    </StackPanel>
                    <TextBlock Grid.Column="1" Grid.Row="7" Text="{Binding Path=Thing.CriticalLow}"/>
                    <ContentControl Grid.Column="1" Grid.Row="8" Content="{Binding Path=Thing.CriticalDamageFactor}" ContentTemplate="{StaticResource deltaView}"/>
                    <TextBlock Grid.Column="1" Grid.Row="10" Text="{Binding Path=Thing.ItemMaterial.Name}"/>
                </Grid>
            </StackPanel.ToolTip>
            <uiui:ItemIcon DataContext="{Binding}"/>
            <TextBlock Text="{Binding Path=Thing.Name}"></TextBlock>
            <TextBlock Text="Price:" Margin="8,0,0,0"></TextBlock>
            <TextBlock Text="{Binding Path=Thing.Price.BasePrice}"></TextBlock>
            <TextBlock Text="Weight:" Margin="8,0,0,0"></TextBlock>
            <TextBlock Text="{Binding Path=Thing.Weight}"></TextBlock>
            <TextBlock Text="Size:" Margin="8,0,0,0"></TextBlock>
            <TextBlock Text="{Binding Path=Thing.ItemSizer.Size.Name}"></TextBlock>
            <TextBlock Text="Creature-Size:" Margin="8,0,0,0"></TextBlock>
            <TextBlock Text="{Binding Path=Thing.ItemSizer.ExpectedCreatureSize.Name}"></TextBlock>
        </StackPanel>
    </DataTemplate>

