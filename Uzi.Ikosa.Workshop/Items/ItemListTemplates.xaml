<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:uiui="clr-namespace:Uzi.Ikosa.UI;assembly=Uzi.Ikosa.UI"
                    xmlns:weapons="clr-namespace:Uzi.Ikosa.Items.Weapons;assembly=Uzi.Ikosa"
                    xmlns:materials="clr-namespace:Uzi.Ikosa.Items.Materials;assembly=Uzi.Ikosa"
                    xmlns:magic="clr-namespace:Uzi.Ikosa.Magic;assembly=Uzi.Ikosa"
                    xmlns:wealth="clr-namespace:Uzi.Ikosa.Items.Wealth;assembly=Uzi.Ikosa"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- TextBlock Label -->
    <Style TargetType="TextBlock" x:Key="styleLabel">
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0,0,2,0"/>
    </Style>

    <!-- STACKPANEL STYLE -->
    <Style TargetType="StackPanel" x:Key="styleItemPanel">
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Background" Value="LightGreen"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style TargetType="Label" x:Key="styleILLabel">
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    <Style TargetType="TextBlock" x:Key="styleILTextBlock">
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <!-- WEAPON HEAD -->
    <DataTemplate DataType="{x:Type weapons:WeaponHead}">
        <Grid Margin="2,0">
            <Grid.Resources>
                <ResourceDictionary>
                    <ResourceDictionary.MergedDictionaries>
                        <ResourceDictionary Source="/Uzi.Ikosa.UI;Component/Deltable.xaml"/>
                    </ResourceDictionary.MergedDictionaries>
                    <Style TargetType="TextBlock">
                        <Setter Property="VerticalAlignment" Value="Center"/>
                    </Style>
                    <Style TargetType="ContentControl">
                        <Setter Property="VerticalAlignment" Value="Center"/>
                    </Style>
                    <Style TargetType="ItemsControl">
                        <Setter Property="VerticalAlignment" Value="Center"/>
                    </Style>
                </ResourceDictionary>
            </Grid.Resources>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="0">Damage</TextBlock>
            <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="1">Enhancement</TextBlock>
            <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="2">Attack/Damage</TextBlock>
            <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="3">Critical Low</TextBlock>
            <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="4">Critical Damage</TextBlock>
            <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="5">Damage Types</TextBlock>
            <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="6">Material</TextBlock>
            <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="7">Is Main</TextBlock>
            <TextBlock Grid.Column="1" Grid.Row="0" Text="{Binding Path=SizedDamageRollString}"/>
            <StackPanel Orientation="Horizontal" Grid.Column="1" Grid.Row="1">
                <TextBlock Text="{Binding Path=ListedEnhancement}"/>
                <TextBlock Text="/"/>
                <ContentControl Content="{Binding Path=TotalEnhancement}" ContentTemplate="{StaticResource deltaView}"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Column="1" Grid.Row="2">
                <ContentControl Content="{Binding Path=AttackBonus}" ContentTemplate="{StaticResource deltaView}"/>
                <TextBlock Text="/"/>
                <ContentControl Content="{Binding Path=DamageBonus}" ContentTemplate="{StaticResource deltaView}"/>
            </StackPanel>
            <TextBlock Grid.Column="1" Grid.Row="3" Text="{Binding Path=CriticalLow}"/>
            <TextBlock Grid.Column="1" Grid.Row="4" Text="{Binding Path=CriticalMultiplier}"/>
            <!--<ContentControl Grid.Column="1" Grid.Row="4" Content="{Binding Path=CriticalDamageFactor}" ContentTemplate="{StaticResource deltaView}"/>-->
            <ItemsControl Grid.Column="1" Grid.Row="5" ItemsSource="{Binding Path=DamageTypes}"/>
            <TextBlock Grid.Column="1" Grid.Row="6" Text="{Binding Path=HeadMaterial.Name}"/>
            <TextBlock Grid.Column="1" Grid.Row="7" Text="{Binding Path=IsMainHead}"/>
        </Grid>
    </DataTemplate>

    <!-- DOUBLE WEAPON -->
    <DataTemplate DataType="{x:Type uiui:DoubleMeleeWeaponVM}">
        <StackPanel Orientation="Horizontal" ToolTipService.ShowDuration="30000" Style="{StaticResource styleItemPanel}">
            <StackPanel.Resources>
                <Style BasedOn="{StaticResource styleILTextBlock}" TargetType="TextBlock"/>
                <Style BasedOn="{StaticResource styleILLabel}" TargetType="Label"/>
            </StackPanel.Resources>
            <StackPanel.ToolTip>
                <Grid >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="0">Proficiency</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="1">Wield-Template</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="2">Is Reach</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="3">Finessable</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="4">Using as Two Handed</TextBlock>
                    <TextBlock Style="{StaticResource styleLabel}" Grid.Column="0" Grid.Row="5">Weapon Heads</TextBlock>
                    <TextBlock Grid.Column="1" Grid.Row="0" Text="{Binding Path=Thing.ProficiencyType}"/>
                    <TextBlock Grid.Column="1" Grid.Row="1" Text="{Binding Path=Thing.WieldTemplate}"/>
                    <TextBlock Grid.Column="1" Grid.Row="2" Text="{Binding Path=Thing.IsReachWeapon}"/>
                    <TextBlock Grid.Column="1" Grid.Row="3" Text="{Binding Path=Thing.IsFinessable}"/>
                    <TextBlock Grid.Column="1" Grid.Row="4" Text="{Binding Path=Thing.UseAsTwoHanded}"/>
                    <Border Grid.Column="1" Grid.Row="5" BorderThickness="0.5" BorderBrush="Brown">
                        <ItemsControl ItemsSource="{Binding Path=Thing.AllHeads}" Margin="2">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Horizontal"/>
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                        </ItemsControl>
                    </Border>
                </Grid>
            </StackPanel.ToolTip>
            <uiui:ItemIcon DataContext="{Binding}"/>
            <TextBlock Text="{Binding Path=Thing.Name}"></TextBlock>
            <TextBlock Text="Price:" Margin="8,0,0,0"></TextBlock>
            <TextBlock Text="{Binding Path=Thing.Price.BasePrice}"></TextBlock>
            <TextBlock Text="Weight:" Margin="8,0,0,0"></TextBlock>
            <TextBlock Text="{Binding Path=Thing.Weight}"></TextBlock>
            <TextBlock Text="Size:" Margin="8,0,0,0"></TextBlock>
            <TextBlock Text="{Binding Path=Thing.ItemSizer.Size.Name}"></TextBlock>
            <TextBlock Text="Creature-Size:" Margin="8,0,0,0"></TextBlock>
            <TextBlock Text="{Binding Path=Thing.ItemSizer.ExpectedCreatureSize.Name}"></TextBlock>
        </StackPanel>
    </DataTemplate>

